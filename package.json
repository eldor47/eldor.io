{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md,mdx,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md,mdx,json,yml,yaml}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.3.1",
    "framer-motion": "^12.23.12",
    "next": "^14.2.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.19.122",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "autoprefixer": "^10.4.14",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.31",
    "postcss": "^8.5.6",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,css,yml,yaml}": [
      "prettier --write"
    ]
  },
  "ct3aMetadata": {
    "initVersion": "7.17.0"
  }
}
